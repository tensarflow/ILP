<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="com.espertech.esper.compat.CompatSettings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
	</configSections>
	<appSettings>
		<clear/>
		<add key="PropertyResolutionStyle" node="CASE_SENSITIVE"/>
	</appSettings>
	<startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup>
	<applicationSettings>
		<com.espertech.esper.compat.CompatSettings>
			<setting name="UseDefaultSearchPath" serializeAs="String">
				<node>True</node>
			</setting>
			<setting name="SearchPath" serializeAs="Xml">
				<node>
					<ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
						<string>..\..\..\etc</string>
					</ArrayOfString>
				</node>
			</setting>
			<setting name="DefaultLockType" serializeAs="String">
				<node>MonitorLock</node>
			</setting>
		</com.espertech.esper.compat.CompatSettings>
	</applicationSettings>
	<log4net>
		<!-- Define some output appenders -->
		<appender name="Console" interfaceClass="log4net.Appender.ConsoleAppender">
			<layout interfaceClass="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" node="[NEsper.Tests] %date{ISO8601} [%thread] %-5level %logger - %message%newline"/>
			</layout>
		</appender>
		<appender name="File" interfaceClass="log4net.Appender.FileAppender">
			<param name="File" node="NEsper.IO.Log.txt"/>
			<param name="AppendToFile" node="false"/>
			<layout interfaceClass="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" node="[NEsper.Tests] %date{ISO8601} [%thread] %-5level %logger - %message%newline"/>
			</layout>
		</appender>
		<!-- Setup the root category, add the appenders and set the default priority -->
		<root>
			<level node="INFO"/>
			<appender-ref ref="File"/>
		</root>
		<logger name="com.espertech.esper.compat.MonitorSpinLock">
			<level node="WARN"/>
		</logger>
	</log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging.Core" publicKeyToken="af08829b84f0328e" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-3.4.1.0" newVersion="3.4.1.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging" publicKeyToken="af08829b84f0328e" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-3.4.1.0" newVersion="3.4.1.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
